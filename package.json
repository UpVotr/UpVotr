{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd server && tsc && cd ../ && cross-env NODE_ENV=development node --enable-source-maps server/dist/index.js",
    "dev:debug": "cd server && tsc && cd ../ && cross-env NODE_ENV=development DEBUG=\"upvotr:*\" node --enable-source-maps server/dist/index.js",
    "build": "cd server && tsc && cd ../ && next build",
    "start": "NODE_ENV=production node server/dist/index.js",
    "lint": "next lint",
    "build:config": "json2ts -i util/upvotr.config.schema.json -o server/src/config.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/md5": "^2.3.2",
    "@types/node": "18.15.10",
    "@types/react": "18.0.29",
    "@types/react-dom": "18.0.11",
    "@upvotr/mysql-query-builder": "^1.0.10",
    "mysql2": "^3.2.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.7",
    "@upvotr/node-hmr": "^1.1.6",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "json-schema-to-typescript": "^12.0.0",
    "md5": "^2.3.0"
  }
}
